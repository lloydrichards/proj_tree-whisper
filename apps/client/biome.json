{
  "$schema": "https://biomejs.dev/schemas/2.1.2/schema.json",
  "root": false,
  "extends": "//",
  "linter": {
    "rules": {
      "nursery": {
        "noUnknownAtRule": "off"
      },
      "performance": {
        "noImgElement": "off",
        "noNamespaceImport": "off"
      },
      "style": {
        "noNestedTernary": "off"
      },
      "suspicious": {
        "noArrayIndexKey": "off"
      }
    }
  }
}
